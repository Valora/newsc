        </javaModelGenerator>
        
        <sqlMapGenerator targetProject="com.sc.mapper" targetPackage="src/main/java">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        
        <javaClientGenerator targetPackage="com.sc.mapper" type="ANNOTATEDMAPPER" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
